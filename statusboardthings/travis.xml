<?xml version='1.0' ?>
<!DOCTYPE html>
<t:chloe xmlns:t="http://factorcode.org/chloe/1.0">
  <table>
    <t:bind-each t:name="repos">
      <tr>
        <td width="44">
          <t:if t:code="statusboardthings:success?"> <img style="margin-top: -4px" src="/static/success.png" /></t:if>
          <t:if t:code="statusboardthings:progress?"><img style="margin-top: -4px" src="/static/progress.png" /></t:if>
          <t:if t:code="statusboardthings:fail?">    <img style="margin-top: -4px" src="/static/fail.png" /></t:if>
        </td>
        <td><t:label t:name="slug" /></td>
      </tr>
    </t:bind-each>
  </table>
</t:chloe>
